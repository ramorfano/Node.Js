<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs') %>
<body>
    
    <%- include('./partials/nav.ejs') %>

    <div class="blogs content">
        <h2>All Blogs</h2>
         
        <% if (blogs.length > 0) { %>
            <% blogs.forEach(blog => { %>

                <h3 class="title"><%= blog.title %></h3>
                <p class="snippet"><%= blog.snippet %></p>
        

        <% }) %>
         <%  } else { %>
            <p>Theres No Data To Display....</p>
            <% } %>

    </div>
    <%- include('./partials/footer.ejs') %>
</body>
</html>